config_version=4

_global_script_classes=[  ]
_global_script_class_icons={

}

[application]

config/name="Labyrinth1"
run/main_scene="res://titlescreen/TitleScreen.tscn"
config/icon="res://icon.png"

[autoload]

dir="*res://engine/directions.gd"
MainGlobal="*res://engine/MainG.gd"
itemEntity="*res://engine/itemEntity.gd"
mMainZone="*res://music/MusicMainZone.tscn"
mCredits="*res://music/musicCredits.tscn"
mMenu="*res://music/musicMenu.tscn"

[debug]

gdscript/warnings/unassigned_variable=false
gdscript/warnings/unused_variable=false
gdscript/warnings/unused_class_variable=false
gdscript/warnings/unused_argument=false
gdscript/warnings/void_assignment=false
gdscript/warnings/return_value_discarded=false

[display]

window/size/width=704
window/size/height=400
window/size/test_width=704
window/size/test_height=400
window/per_pixel_transparency/enabled=true
window/handheld/orientation="sensor_landscape"
window/stretch/mode="2d"
window/stretch/aspect="keep"

[input]

